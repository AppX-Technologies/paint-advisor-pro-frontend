{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prop-types": "off",
		"no-unused-vars": "warn",
		"no-mixed-spaces-and-tabs": "off",
		
	}
}
